<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <meta name="csrf-token" content="{{ csrf_token('delete-item') }}">
        <title>{% block title %}Welcome!{% endblock %}</title>

        <link rel="icon" href="{{ asset('images/logo.png') }}">


        <link rel="stylesheet" href="/sources/bootstrap/bootstrap.min.css" onerror="this.onerror=null;this.href='https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css';">
        <link rel="stylesheet" href="/sources/bootstrap/bootstrap-icons.css" onerror="this.onerror=null;this.href='https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.css';">
        <link rel="stylesheet" href="/sources/bootstrap/bootstrap-table.min.css" onerror="this.onerror=null;this.href='https://cdn.jsdelivr.net/npm/bootstrap-table@1.24.1/dist/bootstrap-table.min.css';">
        <link rel="stylesheet" href="/sources/bootstrap/bootstrap-table-filter-control.min.css" onerror="this.onerror=null;this.href='https://cdn.jsdelivr.net/npm/bootstrap-table@1.24.1/dist/extensions/filter-control/bootstrap-table-filter-control.min.css';">
        <link rel="stylesheet" href="/sources/select2.min.css" onerror="this.onerror=null;this.href='https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/css/select2.min.css';">


        <script src="/sources/jquery.min.js" onerror="this.onerror=null;this.src='https://cdn.jsdelivr.net/npm/jquery/dist/jquery.min.js'"></script>
        <script src="/sources/jquery-ui.min.js" onerror="this.onerror=null;this.src='https://code.jquery.com/ui/1.13.2/jquery-ui.min.js'"></script>
        <script src="/sources/floating_ui/core.js" onerror="this.onerror=null;this.src='https://cdn.jsdelivr.net/npm/@floating-ui/core@1.7.0'"></script>
        <script src="/sources/floating_ui/dom.js" onerror="this.onerror=null;this.src='https://cdn.jsdelivr.net/npm/@floating-ui/dom@1.7.0'"></script>


        {% block stylesheets %}{% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}

        <script src="/sources/bootstrap/bootstrap.bundle.min.js" onerror="this.onerror=null;this.src='https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js'"></script>
        <script src="/sources/bootstrap/bootstrap-table.min.js" onerror="this.onerror=null;this.src='https://cdn.jsdelivr.net/npm/bootstrap-table@1.24.1/dist/bootstrap-table.min.js'"></script>
        <script src="/sources/bootstrap/bootstrap-table-ru-RU.min.js" onerror="this.onerror=null;this.src='https://unpkg.com/bootstrap-table@1.21.2/dist/locale/bootstrap-table-ru-RU.min.js'"></script>
        <script src="/sources/bootstrap/bootstrap-table-filter-control.min.js" onerror="this.onerror=null;this.src='https://cdn.jsdelivr.net/npm/bootstrap-table@1.24.1/dist/extensions/filter-control/bootstrap-table-filter-control.min.js'"></script>
        <script src="/sources/select2.min.js" onerror="this.onerror=null;this.src='https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/js/select2.min.js'"></script>
        {% block javascripts %}{% endblock %}
    </body>
</html>
